{
  "name": "fastboot-pool",
  "version": "0.5.0",
  "description": "Manage Ember FastBoot memory usage using process pools",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel lib -d dist",
    "prepublish": "yarn build",
    "pretest": "yarn build",
    "test": "cross-env DEBUG=fastboot-pool mocha --require babel-register \"test/!(fixtures)/**/*-test.js\"",
    "posttest": "yarn lint",
    "prelint": "eslint --version",
    "lint": "eslint lib test",
    "lint:fix": "yarn lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kellyselden/fastboot-pool.git"
  },
  "keywords": [
    "fork",
    "process",
    "pool",
    "ember",
    "fastboot"
  ],
  "author": "Kelly Selden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kellyselden/fastboot-pool/issues"
  },
  "homepage": "https://github.com/kellyselden/fastboot-pool#readme",
  "engines": {
    "node": ">= 6"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "debug": "^4.0.0",
    "express": "^4.15.2",
    "node-debug-helper": "^0.5.0",
    "request": "^2.81.0",
    "yargs": "^12.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "chai": "^4.0.0",
    "cross-env": "^5.0.0",
    "ember-cli": "^3.0.2",
    "ember-cli-fastboot": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-sane": "^0.7.0",
    "eslint-plugin-prefer-let": "^1.0.0",
    "express-generator": "^4.15.0",
    "fastboot": "^2.0.0",
    "fs-extra": "^7.0.0",
    "mocha": "^5.0.0",
    "renovate-config-standard": "^1.0.0",
    "rimraf": "^2.6.1"
  }
}
